			var ddate = d.getDate();
			var dmonth = d.getMonth();
			var n;
			var f;
			if (dmonth==11) {
				n = 94 - ddate;
			}
			else if (dmonth == 0){
				n = 63 - ddate;
			}
			else if (dmonth == 1){
				n = 32 - ddate;
			}
			else if (dmonth == 2){
				n = 3 - ddate;
			}
			else {
				n=-1;
			}
			f = ((n*2.5)+100) + "px";
  			var windowwidth1 = $(window).width();
			if(windowwidth1<1000)
			{
				$("#swater:not(.animated)").animate({
						height: f
